<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Website_c_format_for_wwwDEPRECATED</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT(ISBLANK( Website__c )),
NOT(BEGINS (Website__c,&quot;www.&quot;))
)</errorConditionFormula>
    <errorDisplayField>Website__c</errorDisplayField>
    <errorMessage>Error Message - you&apos;ve attempted to add a Website Name in a non-supported format - please use the following format - www.&lt;domain name&gt;</errorMessage>
</ValidationRule>
