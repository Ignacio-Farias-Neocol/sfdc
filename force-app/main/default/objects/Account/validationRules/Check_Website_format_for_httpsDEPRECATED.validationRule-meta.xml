<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Website_format_for_httpsDEPRECATED</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT(ISBLANK( Website )),
NOT(BEGINS (Website,&quot;https://www&quot;))
)</errorConditionFormula>
    <errorDisplayField>Website</errorDisplayField>
    <errorMessage>you&apos;ve attempted to add a Website Name in a non-supported format - please use the following format - https://www.&lt;domain name&gt;</errorMessage>
</ValidationRule>
