                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Primary_Hot_List__r.MQL_Detail__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>IsConverted_NOT_MWTrial_Formula</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Reparent_Hot_Lists</targetReference>
            </connector>
            <label>TRUE_IsConverted (NOT MW Trial)</label>
        </rules>
    </decisions>
