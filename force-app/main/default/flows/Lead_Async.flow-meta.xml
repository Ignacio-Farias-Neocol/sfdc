    <formulas>
        <name>IsNewPostMQLFormula</name>
        <dataType>Boolean</dataType>
        <expression>AND(
   ISNEW(),
   {!$Record.RecordType.DeveloperName} = &apos;Post_MQL&apos;,
   NOT(CONTAINS({!$Record.CreatedBy.Username} , &apos;marketo&apos;)),
   NOT($Profile.Name = &apos;Apollo: API&apos;),
   NOT(ISPICKVAL( {!$Record.LeadSource}, &apos;ZoomInfo&apos;) )
)</expression>
    </formulas>
    <formulas>
