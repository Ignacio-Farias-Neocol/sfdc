    <formulas>
        <name>AccountOwnerId</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISNULL({!$Record.Primary_Hot_List__c})) &amp;&amp; {!$Record.Primary_Hot_List__r.MQL_Detail__c} = {!HotList_details},{!Get_MW_Settings.Value__c} ,{!Get_Account.OwnerId} )</expression>
    </formulas>
    <formulas>
        <name>IsNew</name>
        <dataType>Boolean</dataType>
        <expression>ISNEW()</expression>
    </formulas>
    <formulas>
        <name>IsNewPostMQLFormula</name>
        <dataType>Boolean</dataType>
        <expression>AND(
   ISNEW(),
   {!$Record.RecordType.DeveloperName} = &apos;Post_MQL&apos;,
   NOT(CONTAINS({!$Record.CreatedBy.Username} , &apos;marketo&apos;)),
   NOT($Profile.Name = &apos;Apollo: API&apos;),
   NOT(ISPICKVAL( {!$Record.LeadSource}, &apos;ZoomInfo&apos;) )
)</expression>
    </formulas>
    <formulas>
        <name>SetAccountType</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.RecordType.DeveloperName} ==  &apos;Partner_Application&apos;, &apos;Partner - Reseller&apos;, IF({!$Record.RecordType.DeveloperName} !=  &apos;Partner_Application&apos; &amp;&amp; (ISBLANK(TEXT({!Get_Account.Type})) || ISNULL(TEXT({!Get_Account.Type}))),CASE({!$Record.Business_Group__c}, &apos;Core&apos;, &apos;Customer&apos;, &apos;MSP&apos;, &apos;Partner - MSP&apos;,&apos;Sonian&apos;,&apos;End User - Sonian&apos;,TEXT({!Get_Account.Type})),TEXT({!Get_Account.Type})))</expression>
    </formulas>
    <formulas>
        <name>SetBypassValidation</name>
        <dataType>Boolean</dataType>
        <expression>If({!$Record.RecordType.DeveloperName}=&apos;Partner_Application&apos; || ((ISPICKVAL({!$Record.Business_Group__c}, &apos;Core&apos;)  || ISPICKVAL({!$Record.Business_Group__c}, &apos;MSP&apos;) || ISPICKVAL({!$Record.Business_Group__c}, &apos;Sonian&apos;) || ISNULL(TEXT({!$Record.Business_Group__c}))) &amp;&amp; {!$Record.RecordType.DeveloperName}&lt;&gt;&apos;Partner_Application&apos; &amp;&amp; (ISBLANK(TEXT({!Get_Account.Type})) || ISNULL(TEXT({!Get_Account.Type})))),true,{!Get_Account.Bypass_Validation__c})</expression>
    </formulas>
    <formulas>
        <name>SetRecordType</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.RecordType.DeveloperName} != &apos;Partner_Application&apos;,CASE(TEXT({!$Record.Business_Group__c}), &apos;MSP&apos;, {!PartnerRecordTypeId}, &apos;Sonian&apos;, {!CustomerRecordTypeId},{!$Record.RecordTypeId}), {!PartnerRecordTypeId})</expression>
    </formulas>
    <formulas>
        <name>SetRenewalPricingMethod</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!$Record.Business_Group__c}, &apos;MSP&apos;) || ISPICKVAL({!$Record.Business_Group__c}, &apos;Sonian&apos;) || ISNULL(TEXT({!$Record.Business_Group__c})) || ISBLANK(TEXT({!$Record.Business_Group__c})), &apos;Same&apos;,TEXT({!Get_Account.SBQQ__RenewalPricingMethod__c} ))</expression>
    </formulas>
    <formulas>
        <name>SetUsageBilling</name>
        <dataType>Boolean</dataType>
        <expression>If((ISPICKVAL({!$Record.Business_Group__c}, &apos;MSP&apos;) || ISNULL(TEXT({!$Record.Business_Group__c}))) &amp;&amp; {!$Record.RecordType.DeveloperName}=&apos;Partner_Application&apos;  &amp;&amp; (ISBLANK(TEXT({!Get_Account.Type})) || ISNULL(TEXT({!Get_Account.Type}))),true,{!Get_Account.Usage_Based_Billing__c})</expression>
    </formulas>
