            </connector>
            <label>Amendment</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>IsAmendment</name>
        <dataType>Boolean</dataType>
        <expression>IF(AND(NOT(ISBLANK({!$Record.SBQQ__AmendedContract__c})), ISBLANK({!$Record.SBQQ__RenewedContract__c})),True,False)</expression>
    </formulas>
    <formulas>
        <name>IsNew</name>
        <dataType>Boolean</dataType>
        <expression>IF(AND((ISBLANK({!$Record.SBQQ__RenewedContract__c})), ISBLANK({!$Record.SBQQ__AmendedContract__c})),True,False)</expression>
    </formulas>
    <formulas>
        <name>IsQuantityIncreased</name>
        <dataType>Boolean</dataType>
        <expression>IF({!Loop_QuoteLine_Records.SBQQ__Quantity__c} - {!Loop_QuoteLine_Records.SBQQ__EffectiveQuantity__c}&gt;0, TRUE,FALSE)</expression>
    </formulas>
    <formulas>
        <name>IsRenewal</name>
        <dataType>Boolean</dataType>
        <expression>IF(AND(NOT(ISBLANK({!$Record.SBQQ__RenewedContract__c})), ISBLANK({!$Record.SBQQ__AmendedContract__c})),True,False)</expression>
    </formulas>
