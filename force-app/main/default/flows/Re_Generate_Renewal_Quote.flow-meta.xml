<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>57.0</apiVersion>
    <decisions>
        <name>Check_Business_Group</name>
        <label>Check Business Group</label>
        <locationX>524</locationX>
        <locationY>401</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Is_MSP</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Opportunity.Business_Group__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MSP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Opportunity.Business_Group__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Core</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>IsEligibleOrder</targetReference>
            </connector>
            <label>Is MSP</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_if_existing_Quote_is_Approved</name>
        <label>Check if existing Quote is Approved</label>
        <locationX>693</locationX>
        <locationY>834</locationY>
        <defaultConnectorLabel>Not Approved</defaultConnectorLabel>
        <rules>
            <name>Approved</name>
            <conditionLogic>(1 AND 2) OR 3</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Active_Contract__r.SBQQ__RenewalOpportunity__r.SBQQ__PrimaryQuote__r.SBQQ__Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Approved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Active_Contract__r.SBQQ__RenewalOpportunity__r.Business_Group__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MSP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Active_Contract__r.SBQQ__RenewalOpportunity__r.Business_Group__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Core</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Insert_In_Auto_Approve_Contract</targetReference>
            </connector>
            <label>Approved</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_Renewal_Quote_Status</name>
        <label>Check Renewal Quote Status</label>
        <locationX>520</locationX>
        <locationY>723</locationY>
        <defaultConnector>
            <targetReference>Set_Ren_Quoted_to_True</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Checked</defaultConnectorLabel>
        <rules>
            <name>Already_Checked</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Active_Contract__r.SBQQ__RenewalQuoted__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Active_Contract__r.All_Subs_RMA__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Ren_Quoted_to_False</targetReference>
            </connector>
            <label>Already Checked</label>
        </rules>
    </decisions>
    <decisions>
        <name>IsEligibleOrder</name>
        <label>IsEligibleOrder</label>
        <locationX>522</locationX>
        <locationY>538</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>checkEligible</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>isEligible</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_Renewal_Quote_Status</targetReference>
            </connector>
            <label>checkEligible</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>isEligible</name>
        <dataType>Boolean</dataType>
        <expression>IF(AND({!$Record.Opportunity.SBQQ__AmendedContract__c}&lt;&gt;NULL,{!$Record.Active_Contract__r.SBQQ__RenewalOpportunity__c}&lt;&gt;NULL,
{!$Record.Active_Contract__r.SBQQ__RenewalOpportunity__r.SBQQ__PrimaryQuote__c}&lt;&gt;NULL),TRUE,FALSE)</expression>
    </formulas>
    <interviewLabel>Re Generate Renewal Quote {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Re Generate Renewal Quote</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>FREE_FORM_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
