        </inputAssignments>
        <object>Auto_Approve_Contract__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordCreates>
    <recordUpdates>
        <name>Set_Ren_Quoted_to_False</name>
        <label>Set Ren Quoted to False</label>
        <locationX>260</locationX>
        <locationY>841</locationY>
        <connector>
            <targetReference>Set_Ren_Quoted_to_True</targetReference>
        </connector>
        <inputAssignments>
            <field>SBQQ__RenewalQuoted__c</field>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.Active_Contract__r</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Set_Ren_Quoted_to_True</name>
        <label>Set Ren Quoted to True</label>
        <locationX>547</locationX>
        <locationY>1004</locationY>
        <connector>
            <targetReference>Check_if_existing_Quote_is_Approved</targetReference>
        </connector>
        <inputAssignments>
            <field>SBQQ__RenewalQuoted__c</field>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.Active_Contract__r</inputReference>
    </recordUpdates>
    <start>
        <locationX>401</locationX>
        <locationY>48</locationY>
        <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Active_Contract__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>SBQQ__Contracted__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Order</object>
        <recordTriggerType>Update</recordTriggerType>
        <scheduledPaths>
            <name>ReGen_Quote_With_Delay</name>
            <connector>
                <targetReference>Check_Business_Group</targetReference>
            </connector>
            <label>ReGen Quote With Delay</label>
            <offsetNumber>2</offsetNumber>
            <offsetUnit>Minutes</offsetUnit>
            <recordField>Contracted_Date_Time__c</recordField>
            <timeSource>RecordField</timeSource>
        </scheduledPaths>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Draft</status>
</Flow>
