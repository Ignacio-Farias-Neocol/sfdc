    <formulas>
        <name>CaseAccountName</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.AccountId} != null, {!$Record.Account.Name}, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>CaseAccountRegion</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.AccountId} != null &amp;&amp; {!$Record.Account.Terr_Theater__c} != null, {!$Record.Account.Terr_Theater__c}, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>CaseContact</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.ContactId} != null , {!$Record.Contact.FirstName} + &quot;  &quot; + {!$Record.Contact.LastName}, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>ContactPhone</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.Contact.Phone}!= null , {!$Record.Contact.Phone}, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>FirstDayofCurrentWeek</name>
        <dataType>Date</dataType>
        <expression>TODAY() - MOD(TODAY()- DATE(2021,4,5),7)</expression>
    </formulas>
    <formulas>
        <name>FormulaBreak</name>
        <dataType>String</dataType>
        <expression>BR()</expression>
    </formulas>
    <formulas>
        <name>HighTouchAccount</name>
        <dataType>Boolean</dataType>
        <expression>IF (AND(NOT(ISNULL({!$Record.AccountId})), ISPICKVAL({!$Record.Account.Customer_Touch_Category__c}, &quot;High Touch&quot;)) , TRUE,FALSE)</expression>
    </formulas>
    <formulas>
        <name>PremiumAccount</name>
        <dataType>Boolean</dataType>
        <expression>IF({!$Record.AccountId} != null, {!$Record.Account.Premium_Support__c}, false)</expression>
    </formulas>
    <formulas>
        <name>PrimaryProductFamily</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.ProductId} != null, TEXT({!$Record.Product_Family_List__c}),&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>SupportTask</name>
        <dataType>String</dataType>
        <expression>{!$Label.Task_SupportTask_RecordType}</expression>
    </formulas>
    <formulas>
        <name>TaskSubjectFormula</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.Is_Premium_Support__c}==true, &quot;Premium Support outreach Serial # &quot; + {!$Record.Serial__r.Serial_Number__c}, &quot;Support outreach Serial #&quot; +  {!$Record.Serial__r.Serial_Number__c} )</expression>
    </formulas>
    <formulas>
        <name>TechnicalSupportCase</name>
        <dataType>String</dataType>
        <expression>{!$Label.Technical_Support_Record_Type}</expression>
    </formulas>
    <formulas>
