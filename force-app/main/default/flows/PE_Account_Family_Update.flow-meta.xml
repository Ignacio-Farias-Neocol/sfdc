    <formulas>
        <name>fxFamilyType</name>
        <dataType>String</dataType>
        <expression>IF(
{!varMSPCount}&gt;0 &amp;&amp; {!varResellerCount} &gt; 0,
&quot;Partner - MSP ; Partner - Reseller&quot;,
IF(
{!varMSPCount}&gt;0 &amp;&amp; {!varResellerCount} = 0,
&quot;Partner - MSP&quot;,
IF(
{!varMSPCount}=0 &amp;&amp; {!varResellerCount} &gt; 0,
&quot;Partner - Reseller&quot;,
&quot;&quot;
)
)
)</expression>
    </formulas>
