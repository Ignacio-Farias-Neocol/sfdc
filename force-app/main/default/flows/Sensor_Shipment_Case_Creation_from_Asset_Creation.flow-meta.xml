        <label>Check Sensor Products are available or not</label>
        <locationX>638</locationX>
        <locationY>1128</locationY>
        <defaultConnector>
            <targetReference>Loop_Quote_lines</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Sensor_Available</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
