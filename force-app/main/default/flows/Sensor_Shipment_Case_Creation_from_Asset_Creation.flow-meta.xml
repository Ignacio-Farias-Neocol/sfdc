                <leftValueReference>Loop_Quote_lines.SBQQ__ProductName__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Barracuda SKOUT Small Physical Sensor Appliance</stringValue>
                </rightValue>
