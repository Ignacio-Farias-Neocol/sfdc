    <decisions>
        <name>Is_New_Address_from_Community</name>
        <label>Is New Address from Community</label>
        <locationX>182</locationX>
        <locationY>287</locationY>
        <defaultConnectorLabel>Is False Primary and Not from Community</defaultConnectorLabel>
        <rules>
            <name>New_Address_from_Community</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>IsNewAddressFromCommunity</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Status</targetReference>
            </connector>
            <label>New Address from Community</label>
        </rules>
    </decisions>
    <description>This has been removed from Address Management after flow to before flow.</description>
    <environments>Default</environments>
    <formulas>
        <name>Is_Primary_True</name>
        <dataType>Boolean</dataType>
        <expression>OR (
  AND(ISNEW(), {!$Record.Primary__c} = TRUE),
  AND(ISCHANGED({!$Record.Primary__c}), {!$Record.Primary__c} = TRUE)
)</expression>
    </formulas>
    <formulas>
        <name>IsNewAddressFromCommunity</name>
        <dataType>Boolean</dataType>
        <expression>AND (
   ISNEW(),
     BEGINS({!$Profile.Name},  &apos;Barracuda Partner&apos;)
   )</expression>
    </formulas>
