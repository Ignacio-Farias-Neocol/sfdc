    </formulas>
    <formulas>
        <name>Cloned_Oppty_Stage</name>
        <dataType>String</dataType>
        <expression>IF({!Has_Demo_Products} = TRUE, &apos;Tech Evaluation&apos; , &apos;Accepted&apos;)</expression>
    </formulas>
    <formulas>
        <name>Closed_Date</name>
        <dataType>Date</dataType>
        <expression>IF({!Has_Demo_Products} = TRUE, {!Order_Eff_Date}+30, TODAY()+30)</expression>
