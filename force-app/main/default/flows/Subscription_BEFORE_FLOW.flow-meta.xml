    <decisions>
        <description>Check to see if the contract was created by Shivam Sonawane. This user record was used to do data load for SKOUT project. Quotes were not set on the contract on that data load.</description>
        <name>Is_SKOUT_Contract</name>
        <label>Is SKOUT Contract</label>
        <locationX>176</locationX>
        <locationY>791</locationY>
        <defaultConnector>
            <targetReference>Is_Quote_Set_on_the_Contract</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not a SKOUT Contract</defaultConnectorLabel>
        <rules>
            <name>SKOUT_Contract</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Contracts_for_Subscriptions.CreatedById</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>SkoutUserId</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_UniversalIds</targetReference>
            </connector>
            <label>SKOUT Contract</label>
        </rules>
    </decisions>
